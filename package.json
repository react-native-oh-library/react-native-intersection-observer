{
  "name": "@react-native-oh-tpl/react-native-intersection-observer",
  "version": "0.2.0-0.0.1",
  "description": "react native intersection observer",
  "keywords": [
    "react",
    "react-native",
    "intersection",
    "intersection-observer",
    "IntersectionObserver",
    "harmony"
  ],
  "author": "zhbhun",
  "repository": "zhbhun/react-native-intersection-observer",
  "main": "./lib/commonjs/index.js",
  "files": [
    "LICENSE",
    "harmony",
    "lib",
    "src"
  ],
  "harmony": {
    "alias": "react-native-intersection-observer"
  },
  "scripts": {
    "build": "rimraf lib && tsc --build tsconfig.release.json",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bob": "bob build",
    "prepare": "npm run bob"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "react-native-builder-bob": "^0.20.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  }
}
